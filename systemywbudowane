 */
#include "p24FJ128GA010.h"
/*
 * 
 */
int main() {
    LATA = 0;
    TRISA = 0;
    while(1){
        for(unsigned int i = 0x0000; i<=0x00FF;i++)
        {
            LATA=i;
            for(unsigned i = 0; i<65000;i++);
        }
    }
    return (0);
}

/* 
 * File:   main.c
 * Author: local
 *
 * Created on March 24, 2025, 10:34 AM
 */
#include "p24FJ128GA010.h"
/*
 * 
 */
int main() {
    LATA = 0;
    TRISA = 0;
    TRISDbits.TRISD6 = 1;
    TRISDbits.TRISD13 = 1;
    unsigned int program = 0;
    while (1) {
        if (PORTDbits.RD6 == 0) {
            program=program+1;
            while(PORTDbits.RD6 == 0);
            
        }    
        if (PORTDbits.RD13 == 0) {
                program=program-1;
            while(PORTDbits.RD13 == 0);
            
        }
        LATA = program;
    }

    return 0;
}
